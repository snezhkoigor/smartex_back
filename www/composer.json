{
    "name": "smartex/crm",
    "description": "Smartex CRM.",
    "keywords": ["Smartex", "CRM"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "aleksandrzhiliaev/omnipay-advcash": "1.0.0.*",
        "aleksandrzhiliaev/omnipay-payeer": "1.0.*",
        "aleksandrzhiliaev/omnipay-perfectmoney": "1.4.*",
        "barryvdh/laravel-debugbar": "^3.1",
        "barryvdh/laravel-dompdf": "^0.8.2",
        "barryvdh/laravel-snappy": "^0.4.1",
        "delatbabel/elocryptfive": "1.5.*",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "~3.3",
        "fzaninotto/faker": "^1.7",
        "guzzlehttp/guzzle": "6.3.*",
        "intervention/image": "2.4.*",
        "lanin/laravel-api-debugger": "3.1.*",
        "laravel/framework": "5.5.*",
        "laravel/passport": "^4.0",
        "laravel/tinker": "~1.0",
        "niklasravnsborg/laravel-pdf": "^2.0",
        "optimus/api-consumer": "0.2.*",
        "spatie/laravel-activitylog": "2.3.*",
        "spatie/laravel-fractal": "5.3.*",
        "zizaco/entrust": "1.9.*",
        "adiesel82/geoservice-laravel": "0.0.5"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4",
        "filp/whoops": "~2.0",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "^1.1",
        "phpunit/phpunit": "~6.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories",
            "app/Models"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-install-cmd": [
            "ADiesel82\\GeoService\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan optimize",
            "ADiesel82\\GeoService\\ComposerScripts::postUpdate"
        ],
        "test" : [
            "vendor/bin/phpunit"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
